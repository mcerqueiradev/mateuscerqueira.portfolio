{
  "version": 2,
  "routes": [
    {
      "src": "/projects/(.*\\.(json|png|jpg|jpeg|gif|svg))",
      "dest": "/projects/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*\\.(pdf))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|css|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/projects/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}