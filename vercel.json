{
  "routes": [
    {
      "src": "/projects/(.*\\.(png|jpg|jpeg|gif|svg))",
      "dest": "/projects/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(json|pdf|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.\\w+$)",
      "dest": "/$1"
    },
    {
      "src": "/projects/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}