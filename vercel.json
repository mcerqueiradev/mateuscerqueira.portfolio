{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/projects/(.*\\.(png|jpg|jpeg|gif|svg))",
      "dest": "/projects/$1"
    },
    {
      "src": "/(.*\\.pdf)",
      "dest": "/$1"
    },
    {
      "src": "/projectsData.json",
      "dest": "/projectsData.json",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*\\.\\w+$)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}